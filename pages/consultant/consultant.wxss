
page{
  background-color: #e1e9f6
}
.tabbar{
   width: 100%;
   height: 70rpx;
}

.tabbar-item{
  width: 25%;
  float: left;
}

.tabbar-items{
  width: 70%;
  text-align: center;
  line-height: 70rpx;
  margin: 0 auto;
}
.add{
  border-bottom: 2rpx solid #d3bfdc;
  color: #000
}

.con{
  margin:39rpx 30rpx; 
  width: 650rpx;
  display: flex;
  color: #d3bfdc;
  font-size: 32rpx;
}

.lists{
  width: 100%;
  padding: 30rpx;
  border-bottom: 2rpx solid #eee;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.lists-top{
  width: 655rpx;
  /* height: 30rpx; */
  display: flex;
  justify-content: space-between;
}
.lists-data{
  /* margin-left: 30rpx; */
  width: 330rpx;
}
.lists>image{
  width: 140rpx;
  height: 140rpx;
  background-color: #ccc;
  border-radius: 50%;
  border: 2rpx solid #eee;
}
.lists-data>view:nth-child(1){
  margin-top: 10rpx;
  font-size: 32rpx;
  font-weight: 500;
}
.lists-data>view{
  margin-top: 10rpx;
  font-size: 28rpx;
  text-align: justify;
}
.label{
  width: 350rpx ;
  margin: 17rpx 0rpx;
  display: flex;
  justify-content: flex-start;
}
.label>view{
  width: 107rpx;
  height: 26rpx;
  line-height: 26rpx;
  margin-right: 10rpx;
  background-color: #e5d9ea;
  color: #d3bfdc;
  font-size: 20rpx;
  border-radius: 15rpx;
  text-align: center;
}

.lists-content{
  font-size: 25rpx;
  color: #707070;
  line-height: 30rpx;
}

.lists-bottom{
  width: 640rpx;
  display: flex;
  justify-content: space-between;
}

.lists-mean{
  font-size: 25rpx;
  margin-left: 10rpx;
}

.lists-mean>view:first-child{
  color: #595959;
}

.lists-mean>view:last-child{
  color: #d3bfdc;
}

.yuyue{
  width: 172rpx;
  height: 40rpx;
  line-height: 40rpx;
  color:#fff;
  font-size: 28rpx;
  text-align: center;
  border-radius: 15rpx;
  background-color: #d3bfdc;
  margin-top: 20rpx;
} 

.sex{
  width: 100%;
  height: 100rpx;
}
.star{
  width: 100%;
  height: 60rpx;
  display: flex;
  justify-content: flex-start;
  margin-left: -5rpx;

}

.btn{
  /* padding:5rpx 0rpx; */
  /* margin: auto ; */
  background-color: #d3bfdc;
  color:#fff;
  width: 125rpx;
  height:47rpx;
  line-height:47rpx;
  border-radius: 15rpx;
  text-align: center;
  /* margin-left: 30rpx; */
}

.tag_name{
width: 100%;
display: flex;
  justify-content: flex-start;
  font-size: 22rpx;
  /* 自动换行 */
  flex-wrap:wrap;
}
.tag_name>view{
  width: auto;
  padding:0rpx 10rpx;
  margin-right: 20rpx;
  background-color: #f4c49a;
  border-radius: 15rpx;
  color:#fff;
  margin-top: 10rpx;
  font-size: 24rpx
}
.list{
  margin-bottom: 30rpx;
}


.addtabbar{
  position: fixed;
   background-color: #d3bfdc;
    z-index:100;
    width:100%;
    top:0;
    color: #fff;
}


.show{
  width: 710rpx;
