@import "/iview/icon/index.wxss";
/* @import "../../../templates/mytemplate/mytemplate.wxss"; */
.container-box {
  background-color: rgba(0,0,0,.8);
  background-repeat: no-repeat;
  background-size: cover;
  height: 100vh;
  overflow: hidden;
}

.camera-box {
  width: 100vw;
  height: 100%;
}


.room-title {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  color: #666;
  font-size: 16px;
  height: 60rpx;
  line-height: 60rpx;
}
.reset-bg{
  background: #000;
}
.hd-box{
  display: flex;
  box-sizing: border-box;
  justify-content: center;
  position: absolute;
  z-index: 99;
  width: 100%;
  left: 0;
  top: 25%;
}
.hd-img{
  color: #fff;
  font-size:28rpx;
  text-align: center;
}
.hd-img:nth-child(2){
   margin-left:20rpxï¼›
}
.hd-img>.img{
  width: 140rpx;
  height: 140rpx;
  display: block;
  margin:0 auto 30rpx;
}